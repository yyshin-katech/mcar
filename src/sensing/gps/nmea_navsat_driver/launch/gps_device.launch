<?xml version="1.0" encoding="UTF-8"?>

<launch>
  <arg name="param_file_name" doc="name of param file, e.g. rover" default="propak" />
  <arg name="output" default="screen" />
  <arg name="respawn" default="true" />
  <arg name="respawn_delay" default="30" />
  <arg name="clear_params" default="true" />

  <node pkg="nmea_navsat_driver" type="nmea_serial_driver" name="nmea_serial_driver"
        output="$(arg output)"
        clear_params="$(arg clear_params)"
        respawn="$(arg respawn)"
        respawn_delay="$(arg respawn_delay)">
    <rosparam command="load"
              file="$(find nmea_navsat_driver)/config/$(arg param_file_name).yaml" />
  </node>
</launch>

